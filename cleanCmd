BEGIN{
	pat="([0-9]+) ([0-9]+)"
}
match($0,pat,n){
	print "Cleaning simulation number "n[1]", with timestamp of: "n[2]
}
END{
	if(n[1]){
		system("awk -v n="n[1]" -f removeNL simuList > simuList_TMP")
	} else {
		system("awk -v n=0 -f removeNL simuList > simuList_TMP")
	}
	
	system("rm simuList && mv simuList_TMP simuList && rm doneSimuList")
}
